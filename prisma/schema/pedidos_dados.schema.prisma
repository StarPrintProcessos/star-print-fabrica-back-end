model pedidos_dados {
  codigo_pedido          Int      @id
  cliente_codigo         Int?
  cliente_nome_fantasia  String?
  cliente_razao_social   String?

  data_pedido            DateTime?
  data_baixa_pcp         DateTime?
  data_finalizacao       DateTime?
  data_previsao          DateTime?

  atraso                 Boolean?
  divergencia_dias_uteis Int?
  atraso_investigacao    String?
  atraso_motivo          String?
  atraso_desconsiderar   Boolean?

  descricao              String?

  eh_amostra             Boolean?
  acabamento_cold        Boolean?
  retrabalho             Boolean?
  redimensionamento      Boolean?
  desconsiderar_metragem Boolean?

  algo_novo              String[]
  algo_novo_texto        String?

  cores                  Int?
  pistas_de_corte        Int?
  metros_por_pacote      Float?
  quantidade_por_pacote  Int?
  metragem_linear        Float?
  unidades_produzidas    Int?

  perda_investigacao     String?
  perda_motivo           String?
  perda_justificativa    String?

