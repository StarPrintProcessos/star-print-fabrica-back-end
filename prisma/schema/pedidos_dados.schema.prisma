  largura_material_mm    Int?
  nome_materia_prima     String?
  tipo_materia_prima     String?

  codigo_faca            String?
  tipo_faca              String?
  largura_faca           Float?
  altura_faca            Float?
  colunas_faca           Int?
  fator_faca             Float?
  numero_de_dentes_faca  Int?
  l1_corte_faca          Int?

  operacoes              Json?
  maquina_impressao      String?
  maquina_corte          String?
  maquina_revisao        String?
  
  segmento               String?
  metragem_total_m2      Float?
  producao_m2            Float?
  perda_m2               Float?
  perda_percentual       Float?
  
  
  nc_checklist           Boolean?
  nc_rastreio            Boolean?

  validado               Boolean?

  observacao             String?
  pasta_de_arquivamento  String?

  @@map("pedidos_dados")
}